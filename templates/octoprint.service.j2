[Unit]
Description=The snappy web interface for your 3D printer
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
User={{ octoprint.user }}
ExecStart={{ octoprint.dir }}/venv/bin/octoprint serve \
    -c {{ octoprint.dir }}/config.yml \
    --port {{ octoprint.port }}
[Install]
WantedBy=multi-user.target